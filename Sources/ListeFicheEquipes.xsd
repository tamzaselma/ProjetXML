<xs:element name="resume" type="bastri:resume" minOccurs="0" maxOccurs="2"/>
</xs:sequence>
<xs:attribute name="siid" type="xs:string"/>
<xs:attribute name="numnatstruct" type="xs:string" use="optional"/>
</xs:complexType>
</xs:element>
<xs:complexType name="domaine">
